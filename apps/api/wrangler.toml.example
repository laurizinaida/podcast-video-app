name = "podcast-video-api"
main = "src/index.ts"
compatibility_date = "2024-01-01"
compatibility_flags = ["nodejs_compat"]

[vars]
ENVIRONMENT = "development"

[[d1_databases]]
binding = "DB"
database_name = "podcast-video-db"
database_id = "local"

[env.production]
[[env.production.d1_databases]]
binding = "DB"
database_name = "podcast-video-db"
# database_id will be set when deploying to production